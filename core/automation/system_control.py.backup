    def _optimize_memory(self) -> str:
        """Optimize memory usage"""
        try:
            # Clear system caches
            subprocess.run(['sudo', 'purge'], capture_output=True)
            return "âœ… Memory caches cleared"
        except:
            return "âš ï¸ Memory optimization attempted"

    def _optimize_disk(self) -> str:
        """Optimize disk usage"""
        try:
            # Clean temporary files
            temp_dirs = ['/tmp']
            cleaned_files = 0
            
            for temp_dir in temp_dirs:
                try:
                    for file_path in Path(temp_dir).rglob('*'):
                        if file_path.is_file() and file_path.stat().st_size == 0:
                            file_path.unlink()
                            cleaned_files += 1
                except:
                    pass
            
            return f"âœ… Cleaned {cleaned_files} temporary files"
        except:
            return "âš ï¸ Disk optimization attempted"

    def _optimize_processes(self) -> str:
        """Optimize running processes"""
        try:
            # Get process list
            result = subprocess.run(['ps', 'aux'], capture_output=True, text=True)
            process_count = len(result.stdout.split('\n')) - 1
            return f"âœ… Analyzed {process_count} processes"
        except:
            return "âš ï¸ Process optimization attempted"

